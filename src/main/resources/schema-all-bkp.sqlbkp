DROP TABLE match IF EXISTS;

CREATE TABLE match  (
    id BIGINT IDENTITY NOT NULL PRIMARY KEY,
    city VARCHAR(100),
    date DATE,
    player_of_match VARCHAR(100),
    venue VARCHAR(100),
    team1 VARCHAR(100),
    team2 VARCHAR(100),
    toss_winner VARCHAR(100),
    toss_decision VARCHAR(100),
    match_winner VARCHAR(100),
    result VARCHAR(100),
    result_margin VARCHAR(100),
    umpire1 VARCHAR(100),
    umpire2 VARCHAR(100)
);